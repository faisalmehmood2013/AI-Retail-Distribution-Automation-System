      "parameters": {
        "jsCode": "// Get product data\nconst rawProduct = $node[\"New Product Data\"].json;\n\n// Normalize keys by trimming spaces from Google Sheets columns\nlet product = {};\nfor (const key in rawProduct) {\n  product[key.trim()] = rawProduct[key];\n}\n\n// Load customers\nlet customers = $node[\"Customer Detail\"].json || [];\nif (!Array.isArray(customers)) customers = [customers];\n\n// Map final output\nreturn customers.map(customer => ({\n  json: {\n    name: customer[\"Customer Name\"] || \"there\",\n    email: customer[\"Email\"] || \"shahfaisal1234@gmail.com\",\n\n    // Now using trimmed keys\n    productName: product[\"Product Name\"] || \"New Product\",\n    category: product[\"Category\"] || \"General\",\n    price: product[\"Price\"] || \"0\",\n    shortDescription: product[\"Product Description\"] || \"No description\"\n  }\n}));\n\n\n\n"
      },
